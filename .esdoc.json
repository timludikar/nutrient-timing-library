{
  "source": "./src",
  "destination": "./docs",
  "includes": ["\\.js$"],
  "excludes": ["\\.config\\.js$"],
  "plugins": [
    {"name": "esdoc-standard-plugin", "option": {"accessor": {"access": ["public"], "autoPrivate": true}}},
    {"name": "esdoc-flow-type-plugin", "option": {"enable": true}},
    {"name": "esdoc-importpath-plugin", "option": { "replaces": [{ "from": "^src/", "to": "" }]}}
  ]
}
